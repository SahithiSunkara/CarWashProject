export class Customer{
	
id : number;
firstName: string;
middleName: string;
lastName: string;
email: string;
phoneNo: number;
address : string;
password: string;


constructor(id: number,firstName: string,middleName: string,lastName: string, email: string,  phoneNo: number, address: string, password: string){
this.id = id;
this.firstName = firstName;
this.middleName = middleName;
this.lastName = lastName;
this.email = email;
this.phoneNo = phoneNo;
this.address = address;
this.password = password;

  }
}